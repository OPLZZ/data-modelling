@prefix ex: <http://example.com/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s: <http://schema.org/> .
@prefix schemaExt: <http://example.com/schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Právník 47 let z Prahy 5 hledá práci na HPP...Praxe 18 let na magistrátě smlouvy kupní , nájemní, dispozice s pozemky.... Práce s PC. Spolehlivý, pracovitý, rád se naučím novému, praxe i jako přepážkový prac. ve spořitelně. Prosím jen HPP. Děkuji.

ex:job-posting1 a s:JobPosting ;
  s:employmentType ex:full-time ;
  .

ex:person1 a s:Person ;
  s:birthDate "1966"^^xsd:gYear ;
  s:address ex:address1 ;
  s:seeks ex:job-posting1 ;
  s:alumniOf [
    a s:EducationalOrganization
    # Law school?
  ] .

ex:address1 a s:PostalAddress ;
  s:addressLocality "Praha 5" .

ex:full-time a skos:Concept ;
  skos:prefLabel "Hlavní pracovní poměr"@cs .

ex:municipality a skos:Concept ;
  skos:prefLabel "Municipality"@en, "Magistrát"@cs .

ex:employment1 a [
    rdfs:subClassOf s:Event 
  ] ;
  s:duration "P18Y"^^xsd:duration ;
  schemaExt:employee ex:person1 ;
  schemaExt:employer ex:municipality1
  .

ex:municipality1 a s:Organization ;
  org:classification ex:municipality .

schemaExt:employee rdfs:subPropertyOf s:attendee . # adaptation of s:employee?
schemaExt:employer rdfs:subPropertyOf s:attendee . # adaptation of s:employer?
