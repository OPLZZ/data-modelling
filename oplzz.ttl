@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix oplzz: <http://example.com/oplzz#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix schemaExt: <http://example.com/schemaExt#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

oplzz: a owl:Ontology ;
  rdfs:label "Vocabulary for the application for matching job postings and job seekers"@en ;
  dcterms:creator <http://richard.cyganiak.de/#me>, 
    <http://keg.vse.cz/resource/person/jindrich-mynarz>,
    [
      a foaf:Person ;
      foaf:name "Josef Šlerka"
    ],
    [
      a foaf:Person ;
      foaf:name "Eliška Hutníková"
    ],
    [
      a foaf:Person ;
      foaf:name "Vojtěch Hýža"
    ]
  .

oplzz:JobSeeker a rdfs:Class ;
  rdfs:label "Job seeker"@en ;
  rdfs:subClassOf schema:Person ;
  rdfs:isDefinedBy oplzz:
  .

oplzz:feature a rdf:Property ;
  rdfs:label "feature"@en, "rys"@cs ;
  rdfs:domain [
    a owl:Class ;
    owl:unionOf (schema:JobPosting oplzz:JobSeeker)
  ] ;
  rdfs:range skos:Concept ;
  rdfs:isDefinedBy oplzz:
  .

oplzz:startDate a rdf:Property ;
  rdfs:label "start date"@en, "začátek"@cs ;
  rdfs:domain schemaExt:Experience ;
  rdfs:range xsd:gYear ;
  rdfs:isDefinedBy oplzz:
  .

oplzz:experience a rdf:Property ;
  rdfs:label "experience"@en, "zkušenost"@cs ;
  rdfs:domain oplzz:JobSeeker ;
  rdfs:range schemaExt:Experience ;
  rdfs:isDefinedBy oplzz:
  .
 
oplzz:profession a rdf:Property ;
  rdfs:label "profession"@en, "profese"@cs ;
  rdfs:domain oplzz:JobSeeker ;
  rdfs:range xsd:string ;
  rdfs:isDefinedBy oplzz:
  .
